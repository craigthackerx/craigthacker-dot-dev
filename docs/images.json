---
layout: null
---
{
  "media": [
    {% assign media_files = "" | split: "" %}
    {% for file in site.static_files %}
      {% if file.path contains '/assets/memes/' and (file.extname == '.jpg' or file.extname == '.png' or file.extname == '.gif' or file.extname == '.mp4') %}
        {% assign media_files = media_files | push: file.path %}
      {% endif %}
    {% endfor %}
    {{ media_files | map: "prepend", "'" | map: "append", "'" | join: "," }}
  ]
}
